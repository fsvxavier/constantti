        "php": ">=7.2",      
        "phpmailer/phpmailer": "~6.1",
        "phpseclib/phpseclib": "~2.0"
    },
    "require-dev": {
    },
    "suggest": {
        "phpmailer/phpmailer": "PHPMailer is a full-featured email creation and transfer class for PHP"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    }
}
