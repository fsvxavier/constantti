{
    "name": "fsvxavier/constantti",
    "description": "Constantti Plataform",
    "version": "1.0.0",
    "type": "library",
    "keywords": [
        "framework",
        "library",
        "Constantti"
    ],
    "authors": [
        {
            "name": "Fabricio S V Xavier",
            "email": "fabricio@cityware.com.br",
            "homepage": "http://www.constantti.com.br",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/fsvxavier/constantti.git"
        }
    ],
    "homepage": "http://www.constantti.com.br/",
    "autoload": {
        "psr-4": {
            "Constantti\\": "src/"
        },
        "psr-0": {
